{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_URL": "https://handighana-backend.fly.dev/api",
    "VITE_SOCKET_URL": "https://handighana-backend.fly.dev"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://handighana-backend.fly.dev/api",
      "VITE_SOCKET_URL": "https://handighana-backend.fly.dev"
    }
  },
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
